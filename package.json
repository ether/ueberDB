{
  "name": "ueberdb2",
  "description": "Transform every database into a object key value store",
  "url": "https://github.com/ether/ueberDB",
  "keywords": [
    "database",
    "keyvalue"
  ],
  "author": {
    "name": "The Etherpad Foundation"
  },
  "contributors": [
    {
      "name": "John McLear"
    },
    {
      "name": "spcsser"
    },
    {
      "name": "Peter Martischka"
    }
  ],
  "dependencies": {
    "async": "^3.2.0",
    "cassandra-driver": "^4.5.1",
    "dirty": "^1.1.1",
    "elasticsearch": "^16.7.1",
    "mongodb": "^3.6.3",
    "mssql": "^7.0.0-beta.2",
    "mysql": "2.18.1",
    "nano": "^8.2.2",
    "pg": "^8.0.3",
    "redis": "^3.1.2",
    "rethinkdb": "^2.4.2",
    "simple-git": "^2.4.0"
  },
  "optionalDependencies": {
    "sqlite3": "^5.0.1"
  },
  "devDependencies": {
    "cli-table": "^0.3.1",
    "eslint": "^7.20.0",
    "eslint-config-etherpad": "^1.0.25",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.11.0",
    "mocha": "^7.1.2",
    "randexp": "^0.5.3",
    "wtfnode": "^0.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ether/ueberDB.git"
  },
  "main": "./index",
  "version": "1.4.12",
  "bugs": {
    "url": "https://github.com/ether/ueberDB/issues"
  },
  "homepage": "https://github.com/ether/ueberDB",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha test/test*.js"
  },
  "_npmUser": {
    "name": "johnyma22",
    "email": "john@mclear.co.uk"
  },
  "maintainers": [
    {
      "name": "John McLear",
      "email": "john@mclear.co.uk"
    }
  ],
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/.eslintrc.js"
        ],
        "extends": "etherpad/node"
      },
      {
        "files": [
          "**/*"
        ],
        "excludedFiles": [
          "**/.eslintrc.js"
        ],
        "extends": "etherpad/node"
      },
      {
        "files": [
          "test/**/*"
        ],
        "excludedFiles": [
          "**/.eslintrc.js"
        ],
        "extends": "etherpad/tests/backend",
        "overrides": [
          {
            "files": [
              "test/lib/**/*"
            ],
            "rules": {
              "mocha/no-exports": "off"
            }
          }
        ]
      }
    ],
    "root": true
  },
  "engines": {
    "node": "^10.17.0 || >=11.14.0"
  }
}
